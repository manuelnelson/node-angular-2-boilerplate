{
  "name": "node-angular-2-boilerplate",
  "version": "0.0.0",
  "description": "Starter project for an ES6 RESTful Angular Two API",
  "main": "dist",
  "scripts": {
    "build:backend": "babel src/server --ignore client,webpack,public -s -D -d dist --presets es2015,stage-0",
    "build:dev": "webpack --env=dev --config ./src/webpack/webpack.dev.js",
    "build:prod": "webpack --env=prod --config ./src/webpack/webpack.prod.js",
    "dev": "webpack-dev-server --config ./src/webpack/webpack.dev.js",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelnelson/node-angular-2-boilerplate.git"
  },
  "author": "Emmanuel Nelson <info@emmanuelnelson.com>",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^4.0.0",
    "es6-shim": "^0.35.3",
    "express": "^4.13.3",
    "morgan": "^1.8.0",
    "reflect-metadata": "^0.1.10",
    "resource-router-middleware": "^0.6.0",
    "rxjs": "^5.2.0",
    "typescript": "^2.2.1",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.23.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.1.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.9.2",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "ts-loader": "^2.0.1",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.4.2",
    "typings": "^2.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/manuelnelson/node-angular-2-boilerplate/issues"
  },
  "homepage": "https://github.com/manuelnelson/node-angular-2-boilerplate#readme",
  "keywords": [
    "node",
    "angular",
    "2",
    "boilerplate"
  ]
}
